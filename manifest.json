{
  "developer": {
    "name": "JanRie",
    "url": "https://github.com/jrie"
  },
  "description": "Displays a 'view' link above images from Google image search",
  "homepage_url": "https://github.com/jrie/google-panic-image",
  "manifest_version": 2,
  "name": "GooglePanicImages",
  "version": "1.0.11",
  "permissions": ["<all_urls>"],
  "content_scripts": [
	{
		"matches": ["*://*/search*"],
		"include_globs": ["*.google.*"],
		"js": ["gpi.js"],
		"all_frames": true
	}]
}
