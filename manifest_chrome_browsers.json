{
    "description": "Displays a 'view' link and 'image dimensions' on hover at images of Google image search.",
    "homepage_url": "https://github.com/jrie/googlePanicImage",
    "manifest_version": 3,
    "name": "GooglePanicImages",
    "version": "3.0.4",
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": ["*://*/search*"],
        "include_globs": ["*.google.*"],
        "js": ["gpi.js"],
        "all_frames": true
    }]
}